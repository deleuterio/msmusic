			"SeProvas.getQuestionChemistryDoc = function(subject, tags, cb) {\n\n        var datasources = SeProvas.app.datasources['mongodb'];\n        var mongodb     = datasources.adapter.db;\n        var ObjectID    = datasources.ObjectID;\n\n        NodeOffice.readFile(\"Cinética Química.docx\", function (err, bodyObject) {\n          if(err) cb(err);\n          else {\n\n            var questionsToInsert = [];\n            var questionObj = null;\n            var auxOpt = null;\n            var auxOptText = null;\n            var auxAnswer = null;\n            var content = bodyObject.getChemistryContent();\n\n            for (var i = 0; i < content.length; i++) {\n                var val = content[i];\n\n                if (val.text && val.text.indexOf('undefined') < 0) {\n                    val = val.text;\n\n                    if (val.match(/^([0-9]+\\s-\\s)/g)) {\n                        if (questionObj &&\n                            questionObj.answer && \n                            questionObj.answer !== '***') {\n                            if (questionsToInsert.text && \n                                questionsToInsert.text.indexOf('undefined') >= 0)\n                                console.log(questionsToInsert.text.indexOf('undefined') >= 0)\n\n                            questionsToInsert.push(questionObj);\n                        }\n\n                        auxAnswer = null;\n                        var exams = val.replace(/^([0-9]+\\s-\\s)/g, '').trim();\n                        questionObj = {\n                            exams: [exams.substring(1, exams.length - 1)],\n                            subject: subject,\n                            tags: tags,\n                            type: 'open',\n                            options: null,\n                        };\n                    } else if (!questionObj.answer \n                        && !questionObj.options \n                        && !val.match(/^([a-f|A-F]\\))/g))\n                            questionObj.text += val;\n\n                    if (questionObj.options)\n                        questionObj.type = 'closed';\n\n                    if (val.match(/^([a-f|A-F]\\))/g)) {\n                        if (!questionObj.options) questionObj.options = [];\n                        var opt = _.first(val.match(/^([a-f|A-F]\\))/g));\n                        var option = val.replace(/^([a-f|A-F]\\))/g, '').trim();\n\n                        if (!auxOpt && !auxOptText) {\n                            auxOpt = opt;\n                            auxOptText = option;\n                        } else {\n                            questionObj.options.push(auxOptText);\n                            auxOpt = opt;\n                            auxOptText = option;\n                        }\n                    } else if (auxOpt)\n                        auxOptText += val;\n\n                    if (val.match(/^(:\\s)/g) && content[i + 1].text.match(/^(Gab)/g)) {\n                        auxOpt = null;\n                        auxOptText = null;\n\n                        var answer = val.replace(/^(:\\s)/g, '').trim();\n                        if (answer && !questionObj.options)\n                            questionObj.answer = answer;\n\n                        if (answer && questionObj.options) {\n                            if (answer.match(/([a-z|A-Z])/g)) {\n                                answer = answer.toLowerCase().trim();\n                                questionObj.answer = answer.charCodeAt(0) - 97;\n                            };\n                        };\n\n                        if (!answer) questionObj.answer = '***';\n                    };\n\n                    if (val.match(/^(Gab)/g));\n\n                    if (questionObj.answer === '***') {\n                        if (auxAnswer) auxAnswer = '';\n                        auxAnswer += val;\n                    };\n\n\n                };\n            };\n\n            cb(null, questionsToInsert);\n          }\n        });\n    },\n\n    SeProvas.remoteMethod(\n        'getQuestionChemistryDoc', \n        {   \n            accepts: [\n                { arg: 'subject', type: 'string', required: true },\n                { arg: 'tags', type: 'string', required: true },\n            ],\n            returns: [\n                { root: true }\n            ],\n            http: { path: '/getQuestionChemistryDoc', verb: 'post' }\n        }\n    );",
